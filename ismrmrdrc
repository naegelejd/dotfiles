export ISMRMRD_HOME=/usr/local/ismrmrd
export GADGETRON_HOME=/usr/local/gadgetron
export MATLAB_HOME=/Applications/MATLAB_R2013a.app
export GE_ISMRMRD_HOME=/usr/local/ge-ismrmrd
export GE_TOOLS_HOME=/usr/local/ge-tools
export PYTHONHOME=/usr/local/Frameworks/Python.framework/Versions/2.7

export PATH=$ISMRMRD_HOME/bin:$PATH
export PATH=$GE_ISMRMRD_HOME/bin:$PATH
export PATH=$GADGETRON_HOME/bin:$PATH

export DYLD_FALLBACK_LIBRARY_PATH=$ISMRMRD_HOME/lib:$DYLD_FALLBACK_LIBRARY_PATH
export DYLD_FALLBACK_LIBRARY_PATH=$GADGETRON_HOME/lib:$DYLD_FALLBACK_LIBRARY_PATH
export DYLD_FALLBACK_LIBRARY_PATH=$GE_ISMRMRD_HOME/lib:$DYLD_FALLBACK_LIBRARY_PATH
export DYLD_FALLBACK_LIBRARY_PATH=$GE_TOOLS_HOME/lib:$DYLD_FALLBACK_LIBRARY_PATH

export PYTHONPATH=$ISMRMRD_HOME/python:$PYTHONPATH
